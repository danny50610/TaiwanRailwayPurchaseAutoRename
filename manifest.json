{
    "name": "Taiwan Railway Purchase Auto Rename",
    "description": "Auto rename purchase when download from Taiwan Railway website",
    "version": "0.1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://www.railway.gov.tw/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ]
}